{
  "version": 2,
  "builds": [
    {
      "src": "ui/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "ui/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    }
  ],
  "public": "./ui/public"
}
